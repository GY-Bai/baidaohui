{
  "framework": "SvelteKit",
  "css_framework": "Tailwind CSS",
  "external_libraries": [
    "@castlenine/svelte-qrcode",
    "socket.io-client",
    "Supabase"
  ],
  "internal_modules": [
    "$app/navigation",
    "$app/environment",
    "$lib/auth",
    "$lib/utils/auth",
    "$lib/websocket",
    "$lib/email.js",
    "$lib/subdomain-router.ts",
    "$lib/uiTester.ts",
    "$lib/roleSync.ts",
    "$lib/utils/clickOutside.js",
    "$lib/utils/focus-trap.ts"
  ],
  "api_endpoints": {
    "/api/auth/validate": ["GET"],
    "/api/profile/stats": ["GET"],
    "/api/profile/update": ["POST"],
    "/api/user/stats": ["GET"],
    "/api/fortune": ["GET"],
    "/api/fortune/list": ["GET"],
    "/api/fortune/percentile": ["GET"],
    "/api/fortune/reply": ["POST"],
    "/api/fortune/apply": ["POST"],
    "/api/fortune/upload": ["POST"],
    "/api/fortune/modify": ["POST"],
    "/api/fortune/orders": ["GET"],
    "/api/fortune/settings": ["GET", "PUT"],
    "/api/fortune/draft/{orderId}": ["GET"],
    "/api/fortune/upload-reply-image": ["POST"],
    "/api/chat": ["GET"],
    "/api/chat/members": ["GET"],
    "/api/chat/messages": ["GET"],
    "/api/chat/group/general": ["GET"],
    "/api/chat/mark-group-read": ["POST"],
    "/api/chat/cleanup": ["POST"],
    "/api/chat/mark-read": ["POST"],
    "/api/chat/private/permission": ["POST"],
    "/api/chat/private/delete": ["POST"],
    "/api/email/send": ["POST"],
    "/api/keys": ["GET", "POST"],
    "/api/keys/stats": ["GET"],
    "/api/keys/{keyId}": ["DELETE"],
    "/api/keys/{keyId}/toggle-listing": ["POST"],
    "/api/auth/sync-role": ["POST"],
    "/invite/generate": ["POST"],
    "/api/firstmate/operation-logs": ["POST"],
    "/api/sso/session": ["GET"],
    "/api/messages/aggregated": ["GET"],
    "/api/messages/private/{memberId}": ["GET"],
    "/api/messages/send": ["POST"],
    "/api/ecommerce/stores": ["GET"],
    "/api/ecommerce/merchants": ["GET"],
    "/api/profile/master-stats": ["GET"],
    "/api/profile/check-nickname": ["GET", "POST"],
    "/api/profile/update-nickname": ["POST"],
    "/api/products": ["GET"],
    "/api/payment/session": ["POST"]
